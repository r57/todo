{
  "name": "todo-gateway",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@apollo/federation": "^0.13.2",
    "@apollo/gateway": "^0.13.2",
    "@graphql-codegen/cli": "^1.13.0",
    "@graphql-codegen/typescript": "^1.13.0",
    "@graphql-codegen/typescript-resolvers": "^1.13.0",
    "apollo-server": "^2.11.0",
    "graphql": "^14.6.0",
    "graphql-import": "^1.0.0-beta.2",
    "grpc": "^1.24.2",
    "grpc-tools": "^1.8.1",
    "grpc_tools_node_protoc_ts": "^2.5.10",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "proto": "yarn proto_todos_js && yarn proto_todos_ts && yarn proto_users_js && yarn proto_users_ts",
    "proto_todos_js": "yarn run grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated/pb --grpc_out=./src/generated/pb -I=../proto ../proto/*.proto",
    "proto_todos_ts": "yarn run grpc_tools_node_protoc --plugin=node_modules/.bin/protoc-gen-ts --ts_out=./src/generated/pb -I=../proto ../proto/*.proto",
    "proto_users_js": "yarn run grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated/pb --grpc_out=./src/generated/pb -I=../proto ../proto/*.proto",
    "proto_users_ts": "yarn run grpc_tools_node_protoc --plugin=node_modules/.bin/protoc-gen-ts --ts_out=./src/generated/pb -I=../proto ../proto/*.proto",
    "schema": "rm -R ./generated/schema && cp ../../../schema ./generated/schema && graphql-codegen -c graphql_codegen.yml",
    "start": "ts-node index.ts"
  }
}
